{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-typescript"
  ],
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "vue/attribute-hyphenation": ["off", {
      "ignore": []
    }],
    "vue/v-on-event-hyphenation": ["off", {
      "ignore": []
    }]
  }
}
